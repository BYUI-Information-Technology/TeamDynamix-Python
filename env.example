# TeamDynamix API Credentials

# Environment Selection (production or sandbox)
TDX_ENVIRONMENT=production

# Production Authentication (username/password)
TDX_PRODUCTION_USERNAME=your_production_username
TDX_PRODUCTION_PASSWORD=your_production_password

# Sandbox Authentication (username/password)
TDX_SANDBOX_USERNAME=your_sandbox_username
TDX_SANDBOX_PASSWORD=your_sandbox_password

# Legacy variables (still supported)
TD_USERNAME=your_username
TD_PASSWORD=your_password

# Admin authentication (BEID and WebServicesKey)
TD_BEID=your_beid_guid
TD_WEB_SERVICES_KEY=your_web_services_key_guid

# API base URL (optional, overrides environment selection)
# TD_BASE_URL=https://td.byui.edu/TDWebApi 